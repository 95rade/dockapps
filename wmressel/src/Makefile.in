LIBS = -L/usr/lib @X_LIBS@ @X_PRE_LIBS@ -lX11 @LIBS@ @X_EXTRA_LIBS@
FLAGS = @CFLAGS@ @GTK_CFLAGS@ @DEFS@
OBJS = wmressel.o wmgeneral/wmgeneral.o wmgeneral/misc.o wmgeneral/list.o

.c.o:
	cc $(FLAGS) -c $< -o $*.o

wmressel: $(OBJS)
	cc $(FLAGS) -o wmressel $^ $(LIBS)

all:: wmressel

clean::
	for i in $(OBJS) ; do \
		rm -f $$i ; \
	done
	rm -f wmressel
